pipeline {
    agent any

    stages {
        stage('Workspace setup') {
            steps {
                cleanWs()
                echo 'Workspace is cleaned'
            }
        }
        stage ('Second Stage'){
            parallel{
                    stage('Paralell 1') {
                        steps{
                            echo 'Stage 2 - Parallel 1'
                            }
                    }
                    stage('Paralell 2'){
                        steps{
                            echo 'Stage 2 - Paralell 2'

                        }

                    }
                }
            }
        stage('Workspace teardown'){
            steps {
                cleanWs()
                echo 'Workspace is cleaned'
            }
        }
        }
    }